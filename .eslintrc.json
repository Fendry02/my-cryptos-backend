{
    "extends": ["standard", "prettier"],
    "plugins": ["import", "prettier", "standard"],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-var": 2,
      "no-nested-ternary": 1,
      "no-unneeded-ternary": 1,
      "space-before-function-parent": 0,
      "new-cap": 0,
      "prettier/prettier": 2
    }
  }
  